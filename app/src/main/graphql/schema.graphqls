type Query {
  page(id: ID!): Page
  posts: Posts
}

type Page {
  blocks: [Blocks]!
}

type Blocks {
  id: String
  type: String
  tagName: String
  innerHtml: String
}

type Posts {
  edges: [Edges]
}

type Edges {
  node: Node
}

type Node {
  id: String
  title: String
  content: String
}

schema {
  query: Query
}
